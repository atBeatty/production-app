<%= form_for @production do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :contract %>
    <%= f.text_field :contract %>
    <%= f.label :client_name %>
    <%= f.text_field :client_name %>

    <%= f.collection_select(:producer_id, @producers, :id, :name) %>

        





    <%= f.submit %>
<% end %>
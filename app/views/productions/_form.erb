<%= form_for @production do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :contract %>
    <%= f.text_field :contract %>
        <%= f.fields_for :producer do |producer_fields| %>
        <%= producer_fields.label :producer %>
        <%= producer_fields.text_field :producer %>
    <% end %>





    <%= f.submit %>
<% end %>
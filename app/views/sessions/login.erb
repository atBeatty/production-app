<%= form_tag('/login') do %>
    <%= label_tag 'email'  %>
    <%= text_field_tag 'email' %>
    <%= label_tag 'password' %>
    <%= password_field_tag 'password' %>

    <%= submit_tag 'Login' %>

    <%= button_to "Sign in with Github", '/auth/github' %>
    <%= link_to "Sign in with Github", '/auth/github' %>


<% end %>